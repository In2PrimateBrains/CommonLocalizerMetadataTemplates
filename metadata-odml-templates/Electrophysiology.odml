      </property>
      <property>
        <id>09aba0e5-a331-4ccb-bec8-302f3f65c694</id>
        <name>channelID</name>
        <value></value>
        <definition>Identifier for the channel, to be referenced in the data files&amp;#10;</definition>
        <type>string</type>
      </property>
      <property>
        <id>14d23b0a-3615-4b4e-a834-8ed4aa4db4a1</id>
        <name>channelName</name>
        <value></value>
        <definition>Human readable identifier for non-contact related signal, e.g. Daq internal synchronization signals, behavioral signals, behavioral cues, etc.&amp;#10;</definition>
        <type>string</type>
      </property>
      <property>
        <id>222e713d-38c7-4403-8738-28543625176c</id>
        <name>channelReference</name>
        <value></value>
        <definition>Contact used as physical reference, e.g. Contact_id, physical location (subdural, chamber screw)</definition>
        <type>string</type>
      </property>
      <property>
        <id>e937b666-1be4-4999-a458-c0a30da44f69</id>
        <name>channelSamplingFrequency</name>
        <value></value>
        <definition>Sampling rate of the channel</definition>
        <type>float</type>
      </property>
      <property>
        <id>57252c7f-bedc-42f4-8b7f-2821d74193b1</id>
        <name>channelSamplingFrequencyUnit</name>
        <value></value>
        <definition>Unit of the sampling_frequency, will usually be hz</definition>
        <type>string</type>
      </property>
      <property>
        <id>6443d115-fd3f-4056-a56b-9773b2425a65</id>
        <name>channelStatus</name>
        <value></value>
        <definition>Data quality observed on the channel (good/bad), a small description to be given if bad</definition>
        <type>string</type>
      </property>
      <property>
        <id>6e1cddc5-0070-4b7a-a1eb-70ae739b26a3</id>
        <name>channelStatusDescription</name>
        <value></value>
        <definition>Freeform text description of noise or artifact affecting data quality on the channel</definition>
