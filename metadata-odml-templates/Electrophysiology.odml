  <section>
    <id>c7df438a-7e62-4724-b0db-d02d1d37e859</id>
    <type>n.s.</type>
    <name>Electrophysiology</name>
    <section>
      <id>47a58aa7-ead0-4b7a-8140-a5e7cac6ae69</id>
      <type>n.s.</type>
      <name>DataAcquisitionSystem</name>
      <property>
        <id>22e5750f-2ea0-4b04-a720-c2bf6679b606</id>
        <name>creator</name>
        <value></value>
        <definition>Name of the person who set up the system&amp;#10;</definition>
        <type>string</type>
      </property>
      <property>
        <id>e799f664-41d9-409a-be5c-d383c2dc403d</id>
        <name>deviceSerialNumber</name>
        <value></value>
        <definition>The serial number of the components of the setup, recommended to add serial numbers and versions of ALL components constituting the setup </definition>
        <type>string</type>
      </property>
      <property>
        <id>8ce91c62-1195-436a-af76-2fb52e620450</id>
        <name>institutionAddress</name>
        <value></value>
        <definition>The address of the institution where data is recorded</definition>
        <type>string</type>
      </property>
      <property>
        <id>70378cfb-915f-4e7c-bf6e-cae69ff7ca55</id>
        <name>institutionName</name>
        <value></value>
        <definition>The name of the institution where data is recorded</definition>
        <type>string</type>
      </property>
      <property>
        <id>3dbe6f12-23b6-47a2-b6ea-28760ac6cf7b</id>
        <name>maintainer</name>
        <value></value>
        <definition>Name of the person who maintains the system</definition>
        <type>string</type>
      </property>
      <property>
        <id>250b3db4-08ce-4bb4-bb8d-65dad5de5f09</id>
        <name>manufacturer</name>
        <value></value>
        <definition>Manufacturer of the ephys system (e.g. "OpenEphys”, “Alphaomega",”Blackrock”)</definition>
        <type>string</type>
      </property>
      <property>
        <id>4651b37a-05c2-4465-b403-3f78dc8afc42</id>
        <name>manufacturerModelName</name>
        <value></value>
        <definition>Name of the DAQ System used</definition>
        <type>string</type>
      </property>
      <property>
        <id>0b519cee-e187-4100-9fb0-7079b17733ee</id>
        <name>manufacturerModelVersion</name>
        <value></value>
        <definition>Version of the DAQ System</definition>
        <type>string</type>
      </property>
      <property>
        <id>1ab98e39-573e-45ca-bb54-567bb789bfd0</id>
        <name>powerLineFrequency</name>
        <value></value>
        <definition>Frequency (in Hz) of the power line at the setup location&amp;#10;</definition>
        <type>float</type>
      </property>
      <property>
        <id>d8e32072-4462-47b4-96fa-156da382aed6</id>
        <name>recordingSetup</name>
        <value></value>
        <definition>Custom name of the DAQ system</definition>
        <type>string</type>
      </property>
      <property>
        <id>1a5b2d41-0c40-4eac-ae99-6e66d8a54970</id>
        <name>samplingFrequency</name>
        <value></value>
        <definition>Internal (maximum) sampling frequency of the AR recording (e.g. 24000).&amp;#10;</definition>
        <type>float</type>
      </property>
      <property>
        <id>fa2d9712-2cda-497a-b3db-0d98222953f3</id>
        <name>software</name>
        <value></value>
        <definition>Software suite used for data acquisition</definition>
        <type>string</type>
      </property>
      <property>
        <id>c3a26ef5-69f1-4282-a288-77cd50f4fd82</id>
        <name>softwareVersion</name>
        <value></value>
        <definition>Version of the acquisition software</definition>
        <type>string</type>
      </property>
