    <section>
      <id>a8cecde3-34fc-4893-8685-562e429b4a97</id>
      <type>n.s.</type>
      <name>channel</name>
      <property>
        <id>49d471da-e482-4451-a61c-961522043557</id>
        <name>channelContactID</name>
        <value></value>
        <definition>N/a for non-contact related signal</definition>
        <type>string</type>
      </property>
      <property>
        <id>9fd1fdf6-caec-4d64-8647-e830e198eee5</id>
        <name>channelDescription</name>
        <value></value>
        <definition>Brief free-text description of the channel, or other information of interest. </definition>
        <type>string</type>
      </property>
      <property>
        <id>2ec03076-1317-4ae5-9066-decf3010f58e</id>
        <name>channelGain</name>
        <value></value>
        <definition>Amplification factor applied from signal detection at the contact to the signal stored in the data file</definition>
        <type>string</type>
      </property>
      <property>
        <id>d2bfe772-ad90-49bf-8cba-c609fa0f25ae</id>
        <name>channelGround</name>
        <value></value>
